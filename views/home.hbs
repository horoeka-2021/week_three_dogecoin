<section class="summary">
	{{#each auctions}}
		<div class="card">
			<a href="/auctions/{{id}}">
				<img src="{{photoUrl}}" alt="{{description}}" />
			</a>
			<div class="card-container">
				<h4><b>{{name}}</b></h4>
				<p>{{currentPrice}}</p>
				<p>{{description}}</p>
			</div>
		</div>
	{{/each}}

</section>