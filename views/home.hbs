<section class="summary">
        {{#each auctions}}
        <div class="card">
          <img src="{{photoUrl}}" alt="{{description}}" style="width:100%">
          <div class="container">
              <h4><b>{{name}}</b></h4>
              <p>{{currentPrice}}</p>
              <p>{{description}}</p>
          </div>
        </div>
        {{/each}}
 
</section>
