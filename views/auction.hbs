<section class="auction">
	<div class="auction-details-card">
		<img src="{{auction.photoUrl}}" alt="{{auction.photoUrl}}" />
		<div class="card-container">

			<h4><b>John Doe</b></h4>
			<p>Current Price: {{auction.currentPrice}}</p>
			<p>{{auction.description}}</p>

			<div class="bids">
				{{#each bids}}
					<p>{{bidAmount}}</p>
					<p>{{walletAddress}}</p>
				{{/each}}
			</div>

		</div>

	</div>

</section>
<div>
    <span>{{auction.auctionName}}</span>
    <br>
    <span>Current Price:{{auction.currentPrice}}</span>
    <span>{{auction.description}}</span>
    <span><img src="{{auction.photoUrl}}" alt="an awesome coin"></span>

<a href="/auctions/{{auction.id}}/bid">Bid Here</a>
</div>
<div>
    {{#each bids}}
    <span>Amount: {{bidAmount}}</span>
    <br>
    <span>Address: {{walletAddress}}</span>
    <br>
    {{/each}}
</div> 